@page "/"
@using Web1Apps.Server
@namespace Web1Apps.ServerApp.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Web1Apps.ServerApp</title>
    <base href="~/" />

    <link href="_content/Web1Apps.Server/css/bootstrap/bootstrap.min.css" rel="stylesheet" />

    <link href="_content/Web1Apps.Server/css/site.css" rel="stylesheet" />
    <script src="https://kit.fontawesome.com/273d0585ea.js" crossorigin="anonymous"></script>

    <link href="https://fonts.googleapis.com/css?family=Ubuntu&display=swap" rel="stylesheet">

    <link href="_content/Web1Apps.ComponentsLibrary/leaflet/leaflet.css" rel="stylesheet" />
</head>
<body>
    <app>
        @(await Html.RenderComponentAsync<App>(RenderMode.ServerPrerendered))
    </app>

    <script src="_framework/blazor.server.js"></script>

    <script src="_content/Web1Apps.ComponentsLibrary/leaflet/leaflet.js"></script>
    <script src="_content/Web1Apps.ComponentsLibrary/deliveryMap.js"></script>
</body>
</html>
